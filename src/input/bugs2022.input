testsuite "bugs2022"

testcase "leastAffineMultiple"

pF := PrimeField(7)
fP := FiniteFieldPolynomialPackage(pF)
p := (5*x^5+4*x^4+x^3+6*x^2+6*x+4)::Polynomial(pF)
pr := x^2401+x^343+6*x^7+6*x
testEquals("leastAffineMultiple(p)$fP", "univariate(pr::Polynomial(pF))")

